{
    "name": "forteframework/worker",
    "keywords": [
        "worker",
        "code checker",
        "code transformer"
    ],
    "homepage": "https://www.forteframework.com",
    "description": "ForteFramework Worker: a suite of classes to check and transform files content.",
    "version": "1.1.0",
    "require": {
        "php": ">=8.2",
        "forteframework/stdlib": "dev-master",
        "laminas/laminas-config": "^3.9",
        "laminas/laminas-code": "^4.13",
        "laminas/laminas-dependency-plugin": "^2.6",
        "laminas/laminas-text": "^2.11",
        "laminas/laminas-json": "^3.6",
        "laminas/laminas-validator": "^2.46",
        "laminas/laminas-filter": "^2.34",
        "monolog/monolog": "^3.5",
        "psr/log": "^3.0",
        "symfony/console": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/marcospallanzani/forteframework-stdlib"
        }
    ],
    "autoload": {
        "psr-4": {
            "Forte\\Worker\\": [
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Forte\\Worker\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        },
        "sort-packages": true
    }
}

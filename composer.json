{
    "name": "forteframework/api-generator",
    "keywords": ["api"],
    "homepage": "https://www.forteframework.com",
    "description": "ForteFramework-based APIs generator",
    "version": "1.0.0",
    "require": {
        "php": "^7.1.3",
        "psr/log": ">=1.0.0",
        "symfony/console": "^4.3",
        "zendframework/zend-config": "^3.3",
        "zendframework/zend-code": "^3.3",
        "zendframework/zend-text": "^2.7",
        "zendframework/zend-json": "^3.1",
        "zendframework/zend-validator": "^2.12",
        "zendframework/zend-filter": "^2.9",
        "monolog/monolog": "^1.24",
        "symfony/yaml": "^4.3"
    },
    "autoload" : {
      "psr-4" : {
        "Forte\\Worker\\": ["app"]
      }
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}

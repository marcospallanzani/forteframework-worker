{
    "name": "forteframework/worker",
    "keywords": [
        "worker",
        "code checker",
        "code transformer"
    ],
    "homepage": "https://www.forteframework.com",
    "description": "ForteFramework Worker: a suite of classes to check and transform files content.",
    "version": "1.0.0",
    "require": {
        "php": "^7.4",
        "psr/log": ">=1.0.0",
        "symfony/console": "^4.3",
        "laminas/laminas-config": "^3.3",
        "laminas/laminas-code": "^3.3",
        "laminas/laminas-text": "^2.7",
        "laminas/laminas-json": "^3.1",
        "laminas/laminas-validator": "^2.12",
        "laminas/laminas-filter": "^2.9",
        "monolog/monolog": "^1.24",
        "symfony/yaml": "^4.3",
        "forteframework/stdlib": "master@dev",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:marcospallanzani/forteframework-stdlib.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Forte\\Worker\\": [
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Forte\\Worker\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "mockery/mockery": "^1.2"
    }
}

{
    "name": "forteframework/api-generator",
    "keywords": ["api"],
    "homepage": "https://www.forteframework.com",
    "description": "ForteFramework-based APIs generator",
    "version": "1.0.0",
    "repositories": {
        "dev-package": {
            "type": "path",
            "url": "../ff-worker",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "php": "^7.1.3",
        "forteframework/worker": "1.0.*"
    },
    "autoload" : {
      "psr-4" : {
        "Forte\\Generator\\Api\\": ["app"]
      }
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
